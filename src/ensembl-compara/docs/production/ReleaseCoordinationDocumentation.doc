perl $ENSEMBL_CVS_ROOT_DIR/ensembl-compara/scripts/pipeline/create_mlss.pl =
--method_link_type PROTEIN_TREES --force --collection "murinae" --source "e=
nsembl" --reg_conf $ENSEMBL_CVS_ROOT_DIR/ensembl-compara/scripts/pipeline/p=
roduction_reg_ebi_conf.pl -compara compara_master --release




</pre>=20
</div>
</div></li>
<li class=3D"checked" data-inline-task-id=3D"2447"><p>Add ncRNA-trees metho=
