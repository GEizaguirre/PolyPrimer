	  'pTk.pod' '$(INST_LIB)/Tk/pTk.pod' \
	  'pack.pod' '$(INST_LIB)/Tk/pack.pod' \
	  'palette.pod' '$(INST_LIB)/Tk/palette.pod' \
	  'place.pod' '$(INST_LIB)/Tk/place.pod' \
	  'selection.pod' '$(INST_LIB)/Tk/selection.pod' \
	  'send.pod' '$(INST_LIB)/Tk/send.pod' \
	  'tixWm.pod' '$(INST_LIB)/Tk/tixWm.pod' \
	  'tkvars.pod' '$(INST_LIB)/Tk/tkvars.pod' \
	  'widgets.pod' '$(INST_LIB)/Tk/widgets.pod' 
	$(NOECHO) $(TOUCH) pm_to_blib


