	$(NOECHO) $(ECHO) 'No tests defined for $(NAME) extension.'



# --- MakeMaker ppd section:
# Creates a PPD (Perl Package Description) for a binary distribution.
ppd :
	$(NOECHO) $(ECHO) '<SOFTPKG NAME="Tk-pod" VERSION="">' > Tk-pod.ppd
	$(NOECHO) $(ECHO) '    <ABSTRACT></ABSTRACT>' >> Tk-pod.ppd
	$(NOECHO) $(ECHO) '    <AUTHOR></AUTHOR>' >> Tk-pod.ppd
	$(NOECHO) $(ECHO) '    <IMPLEMENTATION>' >> Tk-pod.ppd
	$(NOECHO) $(ECHO) '        <ARCHITECTURE NAME="x86_64-linux-gnu-thread-multi-5.26" />' >> Tk-pod.ppd
	$(NOECHO) $(ECHO) '        <CODEBASE HREF="" />' >> Tk-pod.ppd
	$(NOECHO) $(ECHO) '    </IMPLEMENTATION>' >> Tk-pod.ppd
	$(NOECHO) $(ECHO) '</SOFTPKG>' >> Tk-pod.ppd


